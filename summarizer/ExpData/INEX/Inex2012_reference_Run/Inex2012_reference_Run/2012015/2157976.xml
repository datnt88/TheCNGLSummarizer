<entry level="1" type="bullet">

 All <village wordnetid="108672738" confidence="0.9508927676800064">
<link xlink:type="simple" xlink:href="../283/19159283.xml">
Seoul</link></village>
·<village wordnetid="108672738" confidence="0.9508927676800064">
<link xlink:type="simple" xlink:href="../103/323103.xml">
Gyeonggido</link></village>
·<village wordnetid="108672738" confidence="0.9508927676800064">
<link xlink:type="simple" xlink:href="../074/68074.xml">
Incheon</link></village>
 buses</entry>
<entry level="1" type="bullet">

 Seoul and Incheon Metropolitan Subway networks</entry>
<entry level="1" type="bullet">

 <name wordnetid="106333653" confidence="0.9508927676800064">
<link xlink:type="simple" xlink:href="../163/5143163.xml">
AREX</link></name>
</entry>
